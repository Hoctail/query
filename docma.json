{
  "src": [
    "src/HClient.js",
    {
      "browser": [
        "browser.js"
      ]
    }
  ],
  "dest": "docs",
  "app": {
    "title": "@hoctail/query - Docs",
    "entrance": "api",
    "routing": "path",
    "server": "github",
    "base": "/query"
  },
  "jsdoc": {
    "access": "public"
  },
  "template": {
    "path": "zebra",
    "options": {
      "title": {
        "label": "@hoctail/query",
        "href": "/query/api/#"
      },
      "navbar": {
        "enabled": true,
        "animations": true,
        "menu": [
          {
            "label": "Docs",
            "iconClass": "fas fa-book",
            "href": "./api"
          },
          {
            "label": "Browser Docs",
            "iconClass": "fas fa-book",
            "href": "./api/browser"
          },
          {
            "label": "GitHub",
            "iconClass": "fab fa-github",
            "href": "https://github.com/Hoctail/query",
            "target": "_blank"
          }
        ]
      }
    }
  }
}
