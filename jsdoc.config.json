{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["./src", "browser.js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "template": "node_modules/docdash",
    "destination": "docs/",
    "access": ["public", "undefined"]
  },
  "docdash": {
    "static": true,
    "search": true,
    "private": true,
    "typedefs": true
  }
}
